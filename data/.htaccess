# حماية مجلد البيانات
<Files "*.json">
    Order Allow,Deny
    Deny from all
</Files>

<Files "*.key">
    Order Allow,Deny
    Deny from all
</Files>

# منع الوصول المباشر للملفات
Options -Indexes

# حماية من الوصول المباشر
<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteRule ^.*$ - [F,L]
</IfModule>